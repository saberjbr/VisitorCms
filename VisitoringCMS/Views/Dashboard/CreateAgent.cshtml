        <div id="page-wrapper">
            <br />
            <br />
            <form class="form-horizontal col-md-6" method="post" enctype="multipart/form-data" role="form">
                <div class="form-body">
                    <div class="form-group">
                        <label class="col-md-3 control-label">نام فروشنده</label>
                        <div class="col-md-9">
                            <input type="text" name="name" class="form-control" placeholder="نام">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">آدرس</label>
                        <div class="col-md-9">
                            <input type="text" name="address" class="form-control" placeholder="آدرس">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">طول جغرافیایی</label>
                        <div class="col-md-9">
                            <input type="text" name="lat" class="form-control" placeholder="">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">عرض جغرافیایی</label>
                        <div class="col-md-9">
                            <input type="text" name="lng" class="form-control" placeholder="">
                        </div>
                    </div>
                </div>
                <div class="form-actions">
                    <div class="row">
                        <div class="col-md-offset-3 col-md-9">
                            <button type="submit" class="btn green">ثبت اطلاعات</button>
                            <button type="button" class="btn default">کنسل</button>
                        </div>
                    </div>
                </div>
            </form>
        </div>
